@model IEnumerable<TimeTracking.Models.Users>
@{
    ViewData["Title"] = "Пользователи";

}
<a asp-action="Create"><img src="https://img.icons8.com/ios/452/add-user-male.png" height="30" width="30" alt="Картинка добавления пользователя(404)" /></a>
            <table border="1" width="100%" height="105%">
                <tr>
                    <th>Почта</th>
                    <th>Фамилия</th>
                    <th>Имя</th>
                    <th>Отчество</th>
                </tr>

                @foreach (var item in Model)
                {
                   
                    <tr>
                        <td><a asp-action="OpenUser" asp-controller="Home" asp-route-id="@item.id">@item.Email</a></td>
                        <td><a asp-action="OpenUser" asp-controller="Home" asp-route-id="@item.id">@item.Surname</a></td>
                        <td><a asp-action="OpenUser" asp-controller="Home" asp-route-id="@item.id">@item.FirstName</a></td>
                        <td><a asp-action="OpenUser" asp-controller="Home" asp-route-id="@item.id">@item.PatroName</a></td>
                       
                        <td>
                            <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.id">
                                <img src="https://img.icons8.com/metro/452/edit.png" width="25" height="25" />
                            </a>
                            <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.id">
                                <img src="http://simpleicon.com/wp-content/uploads/trash.png" width="30" height="30"/>
                            </a>
                        </td>

                    </tr>
                }

            </table>